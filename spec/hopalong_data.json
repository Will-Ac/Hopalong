{
  "version_source": "hopalongv124.html",
  "formulas": [
    {
      "id": "classic_sqrt",
      "name": "Classic (sqrt)",
      "desc": "Wolfram/Martin: x' = y \u2212 sgn(x)\u00b7sqrt(|bx \u2212 c|), y' = a \u2212 x"
    },
    {
      "id": "sqrt_plus_gamma_y",
      "name": "Classic + dy",
      "desc": "x' = y \u2212 sgn(x)\u00b7sqrt(|bx \u2212 c|) + dy, y' = a \u2212 x"
    },
    {
      "id": "sqrt_plus_gamma_x",
      "name": "Classic + dx",
      "desc": "x' = y \u2212 sgn(x)\u00b7sqrt(|bx \u2212 c|) + dx, y' = a \u2212 x"
    },
    {
      "id": "mix_inside",
      "name": "sqrt(|b(x+dy)\u2212c|)",
      "desc": "Mix x,y inside sqrt: x' = y \u2212 sgn(x)\u00b7sqrt(|b(x+dy) \u2212 c|), y' = a \u2212 x"
    },
    {
      "id": "trig_kick_x",
      "name": "Trig kick (sin x)",
      "desc": "x' = y \u2212 sgn(x)\u00b7sqrt(|bx \u2212 c|) + d\u00b7sin(x), y' = a \u2212 x"
    },
    {
      "id": "damped",
      "name": "Damped (|d|)",
      "desc": "x' = (y \u2212 sgn(x)\u00b7sqrt(|b x \u2212 c|))\u00b7(1\u2212|d|), y' = a \u2212 x"
    },
    {
      "id": "y_feedback",
      "name": "Y feedback (a\u2212x+d\u00b7y)",
      "desc": "Keep classic x', add feedback on y: y' = a \u2212 x + d\u00b7y"
    },
    {
      "id": "trig_kick_y",
      "name": "Trig kick (sin y)",
      "desc": "x' = y \u2212 sgn(x)\u00b7sqrt(|b x \u2212 c|) + d\u00b7sin(y), y' = a \u2212 x"
    },
    {
      "id": "classic_plus_yy",
      "name": "Classic + d\u00b7y\u00b2",
      "desc": "x' = y \u2212 sgn(x)\u00b7sqrt(|b x \u2212 c|) + d\u00b7y\u00b2, y' = a \u2212 x"
    },
    {
      "id": "cos_xy_kick",
      "name": "Trig kick (cos(x+y))",
      "desc": "x' = y \u2212 sgn(x)\u00b7sqrt(|bx \u2212 c|) + d\u00b7cos(x+y), y' = a \u2212 x"
    },
    {
      "id": "inside_sin_y",
      "name": "Inside sqrt: x+d\u00b7sin(y)",
      "desc": "x' = y \u2212 sgn(x)\u00b7sqrt(|b(x+d\u00b7sin(y)) \u2212 c|), y' = a \u2212 x"
    },
    {
      "id": "inside_cos_x",
      "name": "Inside sqrt: x+d\u00b7cos(x)",
      "desc": "x' = y \u2212 sgn(x)\u00b7sqrt(|b(x+d\u00b7cos(x)) \u2212 c|), y' = a \u2212 x"
    },
    {
      "id": "softsign_kick",
      "name": "Softsign kick",
      "desc": "x' = y \u2212 sgn(x)\u00b7sqrt(|bx \u2212 c|) + d\u00b7(x/(1+|x|)), y' = a \u2212 x"
    },
    {
      "id": "tanh_kick",
      "name": "Tanh kick",
      "desc": "x' = y \u2212 sgn(x)\u00b7sqrt(|bx \u2212 c|) + d\u00b7tanh(x), y' = a \u2212 x"
    },
    {
      "id": "sign_xy",
      "name": "Sign of (x\u00b7y)",
      "desc": "Use sign(x\u00b7y): x' = y \u2212 sgn(xy)\u00b7sqrt(|bx \u2212 c|), y' = a \u2212 x"
    },
    {
      "id": "double_root",
      "name": "Double-root kick",
      "desc": "x' = y \u2212 sgn(x)\u00b7(sqrt(|bx\u2212c|)+d\u00b7sqrt(|by\u2212c|)), y' = a \u2212 x"
    },
    {
      "id": "xy_coupling",
      "name": "XY coupling",
      "desc": "x' = y \u2212 sgn(x)\u00b7sqrt(|bx \u2212 c|) + d\u00b7(x\u00b7y/50), y' = a \u2212 x"
    },
    {
      "id": "positive_hopalong",
      "name": "Positive Hopalong (+sqrt)",
      "desc": "x' = y + sgn(x)\u00b7sqrt(|bx \u2212 c|), y' = a \u2212 x"
    },
    {
      "id": "sinusoidal_hopalong",
      "name": "Sinusoidal Hopalong (sin)",
      "desc": "x' = y + sin(bx \u2212 c), y' = a \u2212 x"
    },
    {
      "id": "threeply",
      "name": "Threeply (Peters)",
      "desc": "x' = y \u2212 sgn(x)\u00b7|sin(x)cos(b) + d \u2212 x\u00b7sin(a+b+d)|, y' = a \u2212 x"
    },
    {
      "id": "quadrup2",
      "name": "Quadrup-2 (Peters)",
      "desc": "x' = y \u2212 sgn(x)\u00b7sin(ln|bx\u2212c|)\u00b7atan((ln|cx\u2212b|)^2), y' = a \u2212 x"
    },
    {
      "id": "chip",
      "name": "Chip (Peters)",
      "desc": "x' = y \u2212 sgn(x)\u00b7cos((ln|bx\u2212c|)^2)\u00b7atan((ln|bx\u2212c|)^2), y' = a \u2212 x"
    }
  ],
